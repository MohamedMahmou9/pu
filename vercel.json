{
    "version": 2,
    "builds": [
      {
        "src": "index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.py"
      }
    ],
    "env": {
      "TELEGRAM_TOKEN": "@telegram_token",
      "ELEVENLABS_API_KEY": "@elevenlabs_api_key"
    }
  }
  